nticated(){\n    try{\n        const userAuth = yield getCurrentUser();\n        console.log('isUserAuthenticated', userAuth)\n        if (!userAuth) return;\n        yield getSnapshotFromUserAuth(userAuth)\n    }catch (error) {\n        yield put(signInFailure(error))\n    }\n}\n\nexport function* onCheckUserSession() {\n    yield takeLatest(UserActionTypes.CHECK_USER_SESSION, isUserAuthenticated)\n}\n\nexport function* signOut(){\n    try{\n        yield auth.signOut();\n        yield put(signOutSuccess())\n    }catch (error) {\n        yield put(signOutFailure(error))\n    }\n}\n\nexport function* onSignOutStart() {\n    yield takeLatest(UserActionTypes.SIGN_OUT_START, signOut)\n}\n\nexport function* signUpWithEmail({payload: {displayName, email, password}}){\n    try{\n        console.log('signUpWithEmail******', displayName, email)\n        const {user} = yield auth.createUserWithEmailAndPassword(email,password);\n        console.log('signUpWithEmail******', user)\n//        if(userAuth) {\n  //          const user = yield createUserProfileDocument(userAuth, {displayName});\n        yield put(signUpSuccess({ user, additionalData: {displayName}}));\n           // yield put(emailSignInStart({email,password}))\n       // }\n    }\n    catch (error) {\n        yield put(signUpFailure(error))\n    } \n}\n\nexport function* onSignUpWithEmailStart(){\n    yield takeLatest(UserActionTypes.SIGN_UP_START, signUpWithEmail)\n}\n\nexport function* signInAfterSignUp({payload: {user, additionalData}}){\n    console.log('signInAfterSignUp', {user, additionalData})\n    yield getSnapshotFromUserAuth(user, additionalData)\n}\nexport function* onSignUpSuccess(){\n    yield takeLatest(UserActionTypes.SIGN_UP_SUCCESS, signInAfterSignUp)\n}\n\nexport function* userSagas() {\n    yield all([\n        call(onGoogleSignInStart),\n        call(onEmailSignInStart),\n        call(onCheckUserSession),\n        call(onSignOutStart),\n        call(onSignUpWithEmailStart),\n        call(onSignUpSuccess)\n    ])      \n}","/Users/tcloss/projects/react-course/crwn-clothing/src/redux/cart/cart.sagas.js",[],{"ruleId":"245","severity":1,"message":"246","line":12,"column":13,"nodeType":"247","messageId":"248","endLine":12,"endColumn":38},{"ruleId":"249","replacedBy":"250"},{"ruleId":"251","replacedBy":"252"},{"ruleId":"245","severity":1,"message":"253","line":2,"column":8,"nodeType":"247","messageId":"248","endLine":2,"endColumn":14},{"ruleId":"245","severity":1,"message":"254","line":12,"column":10,"nodeType":"247","messageId":"248","endLine":12,"endColumn":14},{"ruleId":"245","severity":1,"message":"255","line":12,"column":16,"nodeType":"247","messageId":"248","endLine":12,"endColumn":41},{"ruleId":"245","severity":1,"message":"256","line":2,"column":10,"nodeType":"247","messageId":"248","endLine":2,"endColumn":14},{"ruleId":"245","severity":1,"message":"254","line":5,"column":9,"nodeType":"247","messageId":"248","endLine":5,"endColumn":13},{"ruleId":"245","severity":1,"message":"257","line":16,"column":60,"nodeType":"247","messageId":"248","endLine":16,"endColumn":69},{"ruleId":"245","severity":1,"message":"254","line":6,"column":10,"nodeType":"247","messageId":"248","endLine":6,"endColumn":14},{"ruleId":"245","severity":1,"message":"258","line":6,"column":16,"nodeType":"247","messageId":"248","endLine":6,"endColumn":32},{"ruleId":"245","severity":1,"message":"259","line":34,"column":36,"nodeType":"247","messageId":"248","endLine":34,"endColumn":52},{"ruleId":"245","severity":1,"message":"260","line":3,"column":10,"nodeType":"247","messageId":"248","endLine":3,"endColumn":34},{"ruleId":"245","severity":1,"message":"261","line":2,"column":10,"nodeType":"247","messageId":"248","endLine":2,"endColumn":19},{"ruleId":"245","severity":1,"message":"262","line":2,"column":21,"nodeType":"247","messageId":"248","endLine":2,"endColumn":52},{"ruleId":"249","replacedBy":"263"},{"ruleId":"251","replacedBy":"264"},{"ruleId":"245","severity":1,"message":"265","line":5,"co